{% extends '::base.html.twig' %}
{% block title %}Organize urls{% endblock %}

{% block body %}
    <table class="table table-hover">
        <tr>
            <th>Url</th>
            <th>Delete</th>
        </tr>
        {% for url in urls %}
            <tr>
                <td>{{ url }}</td>
                <td><a href="{{ path('blog_organize_delete_url', {'id': blog.id, 'key': loop.index0 }) }}">X</a></td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}
